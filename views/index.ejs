<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <% messages.forEach(message => { %>

      <p><%= message.user %></p>
      <p><%= message.text %></p>
      <p> <%= message.added %></p>
      <br>
    <% }); %>

    <li><a href="new">New Message</a></li>

  </body>
</html>
